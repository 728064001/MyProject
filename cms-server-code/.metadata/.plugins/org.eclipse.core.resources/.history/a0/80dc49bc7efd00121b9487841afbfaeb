		<script type="text/javascript">
			$(function(){
				var tab = $("<table width="300px" border="1px"></table>");
				for(var i = 0;i<=5;i++){
					var tr = $("<tr></tr>");
					for(var o=0;o<=5;o++){
						var td = $("<td>adf</td>");
						tr.append(td);
					}
					tab.append(tr);
				}
			});
		</script>
		<div id="t" ></div>
